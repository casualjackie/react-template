{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "parser": "babel-eslint",
  "extends": ["eslint:recommended"],
  "rules": {
    "no-unused-vars": "warn",
    "react/jsx-uses-vars": "error"
  }
}
